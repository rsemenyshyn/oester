<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="0dip" />
            <gradient
                android:startColor="@color/colorGrey"
                android:centerColor="@color/colorGrey"
                android:centerY="0.75"
                android:endColor="@color/colorGrey"
                android:angle="0"
                />
        </shape>
    </item>
    <item android:id="@android:id/secondaryProgress">
        <clip>
            <shape>
                <corners android:radius="0dip" />
                <gradient
                    android:startColor="@color/colorGrey"
                    android:centerColor="@color/colorGrey"
                    android:centerY="0.75"
                    android:endColor="@color/colorGrey"
                    android:angle="0"
                    />
            </shape>
        </clip>
    </item>
    <item
        android:id="@android:id/progress"
        >
        <clip>
            <shape>
                <corners
                    android:radius="0dip" />
                <gradient
                    android:startColor="@color/colorProgressStart"
                    android:centerColor="@color/colorProgressCenter"
                    android:centerY="0.75"
                    android:endColor="@color/colorProgressEnd"
                    android:angle="0"
                    />
            </shape>
        </clip>
    </item>
</layer-list>